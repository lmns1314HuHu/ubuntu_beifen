          if (url.jobname != null){
            li += "<a target='_blank' href='" + encodeURI(url.joburl) + "'> 诚聘" + url.jobname.escapeHTML() + "</a>";
          }
          html += "<li>" + li + "</li>";
        }
        $$("#recommended ul").first().innerHTML=html;
      }
    </script>

